[ignore]
.*/bin/.*
.*/ios/*
.*/node_modules/react-native-image-picker

[include]

[libs]
./flow-typed/libdef.js

[lints]

[options]
include_warnings=true
module.name_mapper.extension='css' -> 'empty/object'
